// This is a MiniLang program

int add(int x, int y){
    return x + y;
}

int subtract(int x, int y){
    return x - y;
}

int main() {
    
    int x = 10;

    int y = 20;

    print(x + " + " + y + " = ");
    int sum = add(x, y);
    print(sum);

    print("\n" + x + " - " + y + " = ");
    int diff = subtract(x, y);
    print(diff);




    return 0;
}